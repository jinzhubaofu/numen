{
  "name": "numen",
  "version": "0.3.0",
  "description": "history api manager",
  "main": "index.js",
  "scripts": {
    "start": "node tools/server.js",
    "build": "rimraf lib && NODE_ENV=production gulp build",
    "lint": "fecs src",
    "test": "NODE_ENV=test karma start ./test/karma.local.conf.js",
    "test-ci": "NODE_ENV=test karma start ./test/karma.travis.conf.js",
    "coveralls": "cat test/coverage/lcov/lcov.info | ./node_modules/.bin/coveralls",
    "docs": "esdoc -c esdoc.json"
  },
  "author": "ludafa@outlook.com",
  "repository": "empty",
  "license": "MIT",
  "readme": "readme.md",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-external-helpers-insert-require": "^1.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "esdoc-es7-plugin": "0.0.3",
    "fecs": "^0.8.7",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-babel-external-helpers": "^2.0.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "rimraf": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "babelBuild": {
    "presets": [
      [
        "es2015",
        {
          "loose": true,
          "modules": "umd"
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "external-helpers",
      "external-helpers-insert-require",
      "transform-es3-property-literals",
      "transform-es3-member-expression-literals"
    ]
  }
}
